<ParameterList name="Prec">

    <Parameter name="Number of blocks" type="int" value="2"/>

    <ParameterList name="ThyraPreconditioner">

        <Parameter name="Preconditioner Type" type="string" value="FROSch"/>

        <ParameterList name="Preconditioner Types">

            <ParameterList name="FROSch">

                <Parameter name="FROSch Preconditioner Type" type="string" value="TwoLevelBlockPreconditioner"/>

                <Parameter name="Recycling" type="bool" value="true"/>				<!-- ???? -->
                <Parameter name="DofsPerNode1" type="int" value="2"/>			<!-- Degree of Freedom e.g. Velocity -->
                <Parameter name="DofsPerNode2" type="int" value="1"/>			<!-- Degree of Freedom e.g. Pressure -->
                <Parameter name="DofOrdering1" type="string" value="NodeWise"/>		<!-- NodeWise: All DOFs for one node so e.g. x-y component -->
                <Parameter name="DofOrdering2" type="string" value="NodeWise"/>		<!-- -->
                
                <Parameter name="Overlap" type="int" value="1"/>				<!-- Overlapping Schwarz: How many neighbouring nodes for interface  -->
                <Parameter name="TwoLevel" type="bool" value="true"/>			<!-- One-level-> Scaling issues, therefore TwoLevel, or ThreeLevel (only for not blocksystems)-->
                
                <Parameter name="Level Combination" type="string" value="Additive"/>        <!--Multiplicative  (less parallizable because steps are seperated but often better results),  Additive ( simultaneusly - add coarse level to one-level additative schwarz preconditioner - parallizable --> <!--  -->
                <Parameter name="OverlappingOperator Type" type="string" value="AlgebraicOverlappingOperator"/> <!-- Only Option available at the moment -->
                
                <Parameter name="CoarseOperator Type" type="string" value="IPOUHarmonicCoarseOperator"/><!--GDSWCoarseOperator,IPOUHarmonicCoarseOperator--> <!-- IPOUHarmonicCoarseOperator is a new implementation allowing for using different coarse operator on different block see below-->
                <Parameter name="Null Space Type" type="string"   value="Stokes"/>		    <!-- Laplace (zerlegung der 1), Elasticty (additional rotation modes), Stokes  -->

                <ParameterList name="AlgebraicOverlappingOperator">
                    <Parameter name="Reuse: Symbolic Factorization" type="bool" value="true"/>          <!-- Solange das Sparsity-Pattern gleich bleibt ist true computational effciient weil das sparisty pattern von der invrsen phi_i^T*A*phi_i nicht neu berechnet wird - sparisty pattern is reused no need of new evaluation of sparse graph layout just fill "old" known positions with new values -->
                    <Parameter name="Combine Values in Overlap" type="string" value="Averaging"/>       <!--Restricted,Averaging,Full--> <!-- Full is additative appraoch that we just sum all vector values regardless of overlaps not nice but often works--> 
                    <Parameter name="Overlapping Operator Combination" type="string" value="Averaging"/><!--Restricted,Averaging,Full--> <!-- ???-->
                    <Parameter name="Adding Layers Strategy" type="string" value="CrsGraph"/>	   <!-- CrsGraph, Matrix -->
                    <Parameter name="Level ID" type="int" value="1"/>				   <!-- maybe not used??-->
                    
                    <ParameterList name="Solver">
                        <Parameter name="SolverType" type="string" value="Amesos2"/>		  <!-- how local matrices are inverted -->
                        
                        <Parameter name="Solver" type="string"   value="klu2"/>                       <!-- Specific direct solver          -->

                    </ParameterList>

                </ParameterList>
                
                <!-- ******************************************  EVERYTHING TO IPOU *****************************************************-->
                <ParameterList name="IPOUHarmonicCoarseOperator">
                    
                    <Parameter name="Store Phi" type="bool" value="true"/>                         <!-- Phi corresponds to R0 and P0 from coarse operator -->
                    
                    <Parameter name="Write phi and problem" type="bool" value="false"/>            <!-- Maybe for visualization --->
                    <Parameter name="Write coarse matrix" type="bool" value="false"/>              <!-- Maybe for visualization --->
                    
                    <Parameter name="Use Triple MatrixMultiply" type="bool" value="false"/>        <!-- for phi^T A phi (or R A P) - implementation for triple matrix multiply           -->
                    <Parameter name="Test Unconnected Interface" type="bool" value="false"/>       <!-- ??? --->
                    
                    <Parameter name="Reuse: Coarse Basis" type="bool" value="true"/>	      <!-- Phi:  Necessary if solution changes a lot -->
                    <Parameter name="Reuse: Coarse Matrix" type="bool" value="false"/>	      <!-- Result of triple multiplication -->
                    
                    <Parameter name="Reuse: Extension Symbolic Factorization" type="bool" value="true"/> 		<!-- ??? -->
                    <Parameter name="Reuse: Coarse Matrix Symbolic Factorization" type="bool" value="true"/>	<!-- same but for coarse space    -->
                    
                    <ParameterList name="Blocks">
                        <ParameterList name="1">
                            <Parameter name="Use For Coarse Space" type="bool" value="true"/>	<!-- ???? -->
                            <ParameterList name="InterfacePartitionOfUnity">
                                <Parameter name="Type" type="string" value="GDSW"/>    	       <!-- Here we set GDSW, RGDSW, GDSWS -->
                                <ParameterList name="GDSW"> 						
                                    <Parameter name="Interface Communication Strategy" type="string" value="CrsGraph"/><!-- CreateOneToOneMap, CrsGraph, CrsMatrix --> <!-- Compution of coarse basis matrix ???-->

                                    <Parameter name="Type" type="string" value="Full"/>		<!-- -->
                                    <ParameterList name="Custom">
                                        <Parameter name="Vertices" type="bool" value="true"/>
                                        <Parameter name="ShortEdges" type="bool" value="true"/> <!-- Zwischen zwei knoten -->
                                        <Parameter name="StraightEdges" type="bool" value="true"/> <!-- in 3D auf einer linie -->
                                        <Parameter name="Edges" type="bool" value="true"/>
                                        <Parameter name="Faces" type="bool" value="true"/>
                                    </ParameterList>
                                </ParameterList>
                            </ParameterList>
                        </ParameterList>
                        <ParameterList name="2">
                            <Parameter name="Use For Coarse Space"                  type="bool"     value="true"/> <!-- ???? -->
                            <ParameterList name="InterfacePartitionOfUnity">
                                <Parameter name="Type" type="string" value="GDSW"/>
                                <ParameterList name="GDSW">
                                    <Parameter name="Interface Communication Strategy" type="string" value="CrsGraph"/>
                                    
                                    <Parameter name="Type" type="string" value="Full"/>
                                    <ParameterList name="Custom">
                                        <Parameter name="Vertices" type="bool" value="true"/>
                                        <Parameter name="ShortEdges" type="bool" value="true"/>
                                        <Parameter name="StraightEdges" type="bool" value="true"/>
                                        <Parameter name="Edges" type="bool" value="true"/>
                                        <Parameter name="Faces" type="bool" value="true"/>
                                    </ParameterList>
                                </ParameterList>
                            </ParameterList>
                        </ParameterList>
                    </ParameterList>
                    
                    <ParameterList name="ExtensionSolver">
                        <Parameter name="SolverType" type="string" value="Amesos2"/>
                        <Parameter name="Solver" type="string" value="Klu"/>
                    </ParameterList>
                    
                    <ParameterList name="Distribution"> <!-- -->
                        <Parameter name="Type" type="string" value="linear"/>
                        <Parameter name="GatheringSteps" type="int" value="1"/>
                        <Parameter name="NumProcs" type="int" value="1"/>
                        <Parameter name="Factor" type="double" value="1.0"/>
                    </ParameterList>
                    
                    <ParameterList name="CoarseSolver">
                        <Parameter name="SolverType" type="string" value="Amesos2"/>
                        <Parameter name="Solver" type="string" value="Klu"/>
                    </ParameterList>
                </ParameterList>
                <!-- ******************************************************************************************************************-->
                
                <ParameterList name="GDSWCoarseOperator">
                    <Parameter name="Store Phi" type="bool" value="false"/>
                    
                    <Parameter name="Write phi and problem" type="bool" value="false"/>
                    <Parameter name="Write coarse matrix" type="bool" value="false"/>
                    
                    <Parameter name="Use Triple MatrixMultiply" type="bool" value="false"/>
                    <Parameter name="Test Unconnected Interface" type="bool" value="false"/>
                    
                    <Parameter name="Reuse: Coarse Basis" type="bool" value="false"/>
                    <Parameter name="Reuse: Coarse Matrix" type="bool" value="false"/>
                    
                    <Parameter name="Reuse: Extension Symbolic Factorization" type="bool" value="true"/>
                    <Parameter name="Reuse: Coarse Matrix Symbolic Factorization" type="bool" value="false"/>
                    
                    <ParameterList name="Blocks">
                        <ParameterList name="1">
                            <Parameter name="Interface Communication Strategy" type="string" value="CreateOneToOneMap"/><!-- CreateOneToOneMap, CrsGraph, CrsMatrix -->
                            <Parameter name="Exclude" type="string" value="2"/>
                            <Parameter name="Use For Coarse Space" type="bool" value="true"/>
                            <Parameter name="Rotations" type="bool" value="false"/>
                        </ParameterList>
                        <ParameterList name="2">
                            <Parameter name="Interface Communication Strategy" type="string" value="CreateOneToOneMap"/><!-- CreateOneToOneMap, CrsGraph, CrsMatrix -->
                            <Parameter name="Exclude" type="string" value="1"/>
                            <Parameter name="Use For Coarse Space" type="bool" value="true"/>
                            <Parameter name="Rotations" type="bool" value="false"/>
                        </ParameterList>
                    </ParameterList>

                    <ParameterList name="ExtensionSolver">
                        <Parameter name="Reuse Symbolic Factorization" type="bool" value="false"/>
                        <Parameter name="SolverType" type="string" value="Amesos2"/>
                        <Parameter name="Solver" type="string" value="klu"/>
                        <ParameterList name="Amesos2">
                            <!-- <ParameterList name="MUMPS">
                                <Parameter name="Reuse SymbolicFactorization" type="bool" value="true"/>
                            </ParameterList> -->
                        </ParameterList>
                    </ParameterList>

                    <ParameterList name="Distribution">
                        <Parameter name="Type" type="string" value="linear"/>
                        <Parameter name="GatheringSteps" type="int" value="1"/>
                        <Parameter name="NumProcs" type="int" value="1"/>
                        <Parameter name="Factor" type="double" value="1.0"/>
                    </ParameterList>

                    <ParameterList name="CoarseSolver">
                        <Parameter name="SolverType" type="string" value="Amesos2"/>
                        <Parameter name="Solver" type="string" value="Klu"/>
                    </ParameterList>

                </ParameterList>

                <ParameterList name="RGDSWCoarseOperator">
                    <Parameter name="Store Phi" type="bool" value="true"/>
                    
                    <Parameter name="Write phi and problem" type="bool" value="false"/>
                    <Parameter name="Write Coarse matrix" type="bool" value="false"/>
                    <!--Write coarse matrix: new impelemtation with small c in coarse-->
                    <Parameter name="Use Triple MatrixMultiply" type="bool" value="false"/>
                    <Parameter name="Test Unconnected Interface" type="bool" value="false"/>
                    
                    <Parameter name="Reuse: Coarse Basis" type="bool" value="false"/>
                    <Parameter name="Reuse: Coarse Matrix" type="bool" value="false"/>
                    
                    <Parameter name="Reuse: Extension Symbolic Factorization" type="bool" value="true"/>
                    <Parameter name="Reuse: Coarse Matrix Symbolic Factorization" type="bool" value="false"/>
                    <ParameterList name="Blocks">
                        <ParameterList name="1">
                            <Parameter name="Interface Communication Strategy" type="string" value="CreateOneToOneMap"/><!-- CreateOneToOneMap, CrsGraph, CrsMatrix -->
                            <Parameter name="Exclude" type="string"   value="2"/>
                            <Parameter name="Use For Coarse Space" type="bool" value="true"/>
                            <Parameter name="Option" type="string"      value="1"/>
                            <Parameter name="Rotations" type="bool"     value="false"/>
                        </ParameterList>
                        <ParameterList name="2">
                            <Parameter name="Interface Communication Strategy" type="string" value="CreateOneToOneMap"/><!-- CreateOneToOneMap, CrsGraph, CrsMatrix -->
                            <Parameter name="Exclude" type="string"   value="1"/>
                            <Parameter name="Use For Coarse Space" type="bool"     value="true"/>
                            <Parameter name="Option" type="string"      value="1"/>
                            <Parameter name="Rotations" type="bool"     value="false"/>
                        </ParameterList>
                    </ParameterList>

                    <ParameterList name="ExtensionSolver">
                        <Parameter name="Reuse Symbolic Factorization" type="bool" value="false"/>
                        <Parameter name="SolverType" type="string"   value="Amesos2"/>
                        <Parameter name="Solver" type="string"   value="klu"/>
                        <ParameterList name="Amesos2">
                            <!-- <ParameterList name="MUMPS">
                                <Parameter name="Reuse SymbolicFactorization" type="bool" value="true"/>
                            </ParameterList> -->
                        </ParameterList>
                    </ParameterList>

                    <ParameterList name="Distribution">
                        <Parameter name="Type" type="string" value="linear"/>
                        <Parameter name="GatheringSteps" type="int" value="1"/>
                        <Parameter name="NumProcs" type="int" value="1"/>
                        <!-- How to use NumProcs
                         -> 0 stands for all processes
                         -> 1 stands for 1 process
                         -> -1 stands corresponds to MaxProcs -1 from Mumps
                         -> n stands for a factor of 1/n
                         -> -n stands for the n-th root
                         -->
                        <Parameter name="Factor" type="double" value="1.0"/>
                        <!-- NumProcs = Factor * NumProcs -->
                        <ParameterList name="Zoltan2 Parameter">
                            <Parameter name="algorithm" type="string"   value="parmetis"/>
                            <Parameter name="debug_level" type="string"   value="basic_status"/>
                            <Parameter name="debug_procs" type="string"   value="0"/>
                            <Parameter name="error_check_level" type="string"   value="basic_assertions"/>
                            <Parameter name="imbalance_tolerance" type="double"   value="1.1"/>
                            <Parameter name="timer_type" type="string"   value="macro_timers"/>
                            <!-- sets num_global_parts = NumProc()/num_global_parts_fraction -->
                        </ParameterList>
                    </ParameterList>

                    <ParameterList name="CoarseSolver">
                        <Parameter name="Reuse Symbolic Factorization" type="bool" value="false"/>
                        <Parameter name="SolverType" type="string" value="Amesos2"/>
                        <Parameter name="Solver" type="string" value="klu"/>
                        <ParameterList name="Amesos2">
                            <!-- <ParameterList name="MUMPS">
                                <Parameter name="Reuse SymbolicFactorization" type="bool" value="true"/>
                            </ParameterList> -->
                        </ParameterList>
                    </ParameterList>

                </ParameterList>

            </ParameterList>
        </ParameterList>
    </ParameterList>
</ParameterList>

